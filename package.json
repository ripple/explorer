{
  "name": "explorer",
  "description": "XRPL Data Visualization",
  "version": "1.5.0",
  "dependencies": {
    "@fontsource/roboto": "^4.5.7",
    "@google-cloud/bigquery": "^5.5.0",
    "@xrplf/prettier-config": "^1.5.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.15",
    "classnames": "^2.3.1",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "d3": "^6.7.0",
    "d3-geo": "^1.11.3",
    "d3-hexbin": "^0.2.2",
    "d3-scale": "^2.2.2",
    "debug": "^4.3.1",
    "dotenv": "^16.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "express": "^4.18.1",
    "i18next": "^21.6.16",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^1.4.0",
    "jest-enzyme": "^7.1.0",
    "moment": "^2.29.4",
    "payid-lib": "^0.1.0",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.0",
    "react-i18next": "^11.16.7",
    "react-json-view": "^1.21.3",
    "react-query": "^3.39.2",
    "react-redux": "^5.0.7",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^4.0.3",
    "recharts": "^2.1.13",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rewire": "^6.0.0",
    "ripple-address-codec": "^4.2.4",
    "topojson-client": "^3.0.0",
    "ws": "^7.4.2",
    "xrpl-client": "^2.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.186",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.9",
    "@types/react-i18next": "^8.1.0",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "canvas": "^2.9.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.8.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "jest-websocket-mock": "^2.3.0",
    "lint-staged": "^13.0.3",
    "mock-socket": "^9.1.2",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react-test-renderer": "^16.14.0",
    "redux-mock-store": "^1.5.1",
    "sass": "^1.51.0",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.9.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!arc)/(?!d3)"
    ],
    "moduleNameMapper": {
      "d3-hexbin": "<rootDir>/node_modules/d3-hexbin/build/d3-hexbin.min.js",
      "d3": "<rootDir>/node_modules/d3/dist/d3.min.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/i18n.js",
      "!src/i18nTestConfig.js",
      "!src/setupTests.js",
      "!src/setupProxy.js",
      "!src/rootReducer.js",
      "!src/registerServiceWorker.js",
      "!src/containers/shared/images/*",
      "!src/containers/test/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "index",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "DISABLE_NEW_JSX_TRANSFORM=true node ./build-non-split.js",
    "dev-client": "react-scripts start",
    "dev-server": "NODE_ENV=development PORT=5001 nodemon --watch server --watch build server|bunyan || true",
    "eject": "react-scripts eject",
    "lint": "run-s \"lint:js -- --fix\" \"lint:css -- --fix\"",
    "lint:ci": "run-s lint:js lint:css",
    "lint:css": "node_modules/.bin/stylelint src/**/*.scss",
    "lint:js": "node_modules/.bin/eslint --ext=js --ext=jsx --ext=ts --ext=tsx --color .",
    "precommit": "lint-staged",
    "prod-server": "node server|bunyan",
    "start": "DISABLE_NEW_JSX_TRANSFORM=true run-p dev-server dev-client",
    "test": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts test --env=jsdom ",
    "test:all": "DISABLE_NEW_JSX_TRANSFORM=true run-s lint:ci test:ci",
    "test:ci": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts test --coverage --ci --color --env=jsdom --no-cache --runInBand",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test:coverage": "npm run test -- --coverage --watchAll=false"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app",
      "plugin:jsx-a11y/recommended",
      "plugin:import/typescript",
      "prettier"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".ts",
            ".tsx",
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ],
      "no-bitwise": "off",
      "react/jsx-props-no-spreading": "warn",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "import/no-cycle": [
        2,
        {
          "maxDepth": 5
        }
      ],
      "import/prefer-default-export": "off",
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ]
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "globals": {
      "ga": true
    }
  },
  "prettier": "@xrplf/prettier-config",
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss",
      "stylelint-config-idiomatic-order",
      "stylelint-config-prettier"
    ],
    "rules": {
      "at-rule-empty-line-before": null,
      "block-closing-brace-newline-after": null,
      "length-zero-no-unit": null
    }
  },
  "engines": {
    "node": ">=14.15.0 <15.0.0",
    "npm": ">=7.14.0 <9.0.0"
  }
}
