{
  "name": "explorer",
  "description": "XRPL Data Visualization",
  "version": "1.5.0",
  "dependencies": {
    "@google-cloud/bigquery": "^5.5.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.15",
    "classnames": "^2.3.1",
    "core-js": "^3.20.2",
    "d3-geo": "^1.11.3",
    "d3-hexbin": "^0.2.2",
    "d3-scale": "^2.2.2",
    "debug": "^4.3.1",
    "dotenv": "^5.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "i18next": "^10.6.0",
    "i18next-browser-languagedetector": "^2.1.1",
    "i18next-xhr-backend": "^1.5.1",
    "intl": "^1.2.5",
    "jest-enzyme": "^7.1.0",
    "localforage": "^1.7.1",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "node-sass-chokidar": "^1.5.0",
    "payid-lib": "^0.1.0",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.0",
    "react-i18next": "^9.0.3",
    "react-json-view": "^1.21.3",
    "react-redux": "^5.0.7",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^4.0.3",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "rewire": "^6.0.0",
    "ripple-address-codec": "^4.1.2",
    "topojson-client": "^3.0.0",
    "typeface-roboto": "0.0.54",
    "ws": "^7.4.2",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.9",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.28.5",
    "canvas": "^2.6.1",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.8.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^0.19.2",
    "husky": "^7.0.4",
    "jest-websocket-mock": "^1.1.1",
    "lint-staged": "^11.0.0",
    "mock-socket": "^9.0.8",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.14.0",
    "redux-mock-store": "^1.5.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.0.6",
    "stylelint-scss": "^3.6.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/i18n.js",
      "!src/i18nTestConfig.js",
      "!src/setupTests.js",
      "!src/setupProxy.js",
      "!src/rootReducer.js",
      "!src/registerServiceWorker.js",
      "!src/containers/shared/images/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "main": "index",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "run-s build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-js": "DISABLE_NEW_JSX_TRANSFORM=true node ./build-non-split.js",
    "dev-client": "react-scripts start",
    "dev-server": "NODE_ENV=development PORT=5001 nodemon --watch server --watch build server|bunyan || true",
    "eject": "react-scripts eject",
    "lint": "run-s lint:js lint:css",
    "lint:css": "node_modules/.bin/stylelint src/**/*.scss --syntax scss",
    "lint:js": "node_modules/.bin/eslint --ext=js --ext=jsx --ext=ts --ext=tsx --color . --fix",
    "precommit": "lint-staged",
    "prod-server": "node server|bunyan",
    "start": "DISABLE_NEW_JSX_TRANSFORM=true run-p watch-css dev-server dev-client",
    "test": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts test --env=jsdom ",
    "test:all": "DISABLE_NEW_JSX_TRANSFORM=true run-s lint:css build-css lint:js test:ci",
    "test:ci": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts test --coverage --ci --color --env=jsdom --no-cache --runInBand",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app",
      "plugin:prettier/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier/react",
      "plugin:import/typescript"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".ts",
            ".tsx",
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ],
      "no-bitwise": "off",
      "react/jsx-props-no-spreading": "warn",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ]
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "globals": {
      "ga": true
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss",
      "stylelint-config-idiomatic-order",
      "stylelint-config-prettier"
    ],
    "rules": {
      "at-rule-empty-line-before": null,
      "block-closing-brace-newline-after": null,
      "length-zero-no-unit": null
    }
  },
  "engines": {
    "node": ">=14.15.0 <15.0.0",
    "npm": ">=7.14.0 <8.0.0"
  }
}
